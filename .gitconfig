  editor = vim
  excludesfile = /home/mlinhares/.gitignore
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
[push]
  default = matching
[url "ikwa@192.168.0.139:"]
  insteadOf = ikwa:
